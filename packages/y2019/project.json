{
  "name": "y2019",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/y2019/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/packages/y2019",
        "main": "packages/y2019/src/main.ts",
        "tsConfig": "packages/y2019/tsconfig.app.json",
        "assets": ["packages/y2019/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "packages/y2019/src/environments/environment.ts",
              "with": "packages/y2019/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "y2019:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "y2019:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/y2019/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/y2019/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:2019", "type:app"]
}
